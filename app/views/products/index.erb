<table>
  <thead>
    <tr>
      <th>商品名</th>
      <th>イメージ</th>
      <th>在庫数</th>
      <th>カテゴリー</th>
      <th>傘骨</th>
      <th>商品アラート数</th>
    </tr>
  </thead>
  <tbody>
    <% @products.each do |product| %>
    <tr>
      <td><%= product.name %></td>
      <td><%= product.image %></td>
      <td><%= product.stock %></td>
      <td><%= product.category.name %></td>
      <td><%= product.frame.name %></td>
      <td><%= product.product_alert.quantity %></td>
    </tr>
    <% end %>
  </tbody>
</table>